{
  "name": "tomatic",
  "version": "1.0.0",
  "description": "FTP - AutoSync, especially for Raspberry Pi Development",
  "main": "main.js",
  "scripts": {
    "watch": "webpack --watch --progress --debug",
    "build": "webpack --progress",
    "app": "electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=src/assets/icons/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . electron-tutorial-app --overwrite --asar=true --platform=win32 --arch=ia32 --icon=src/assets/icons/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Electron Tutorial App\"",
    "package-linux": "electron-packager . electron-tutorial-app --overwrite --asar=true --platform=linux --arch=x64 --icon=src/assets/icons/icon.png --prune=true --out=release-builds"
  },
  "author": "Henrik Engelbrink",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "electron": "^1.7.9",
    "electron-builder": "^19.46.4",
    "css-loader": "^0.28.7",
    "style-loader": "^0.19.0",
    "url-loader": "^0.6.2"
  },
  "dependencies": {
    "async": "^2.6.0",
    "babel": "^6.23.0",
    "chokidar": "^1.7.0",
    "ftp": "^0.3.10",
    "ftp-client": "^0.2.2",
    "moment": "^2.19.2",
    "prop-types": "^15.6.0",
    "react": "^16.1.1",
    "react-dom": "^16.1.1",
    "webpack": "^3.8.1"
  }
}
